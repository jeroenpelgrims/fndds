.open food.db
CREATE TABLE nutrient(
  id INT PRIMARY KEY,
  name VARCHAR NOT NULL,
  unit_name VARCHAR NOT NULL,
  nutient_nbr INT,
  rank INT
);
CREATE TABLE food(
  fdc_id INT PRIMARY KEY,
  data_type VARCHAR NOT NULL,
  description VARCHAR NOT NULL
);
CREATE TABLE food_nutrient(
  fdc_id INT,
  nutrient_id INT,
  amount float,
  CONSTRAINT PK_food_nutrient PRIMARY KEY (fdc_id, nutrient_id),
  FOREIGN KEY(fdc_id) REFERENCES food(fdc_id),
  FOREIGN KEY(nutient_id) REFERENCES nutient(id)
);

.mode csv
.import data/nutrient.csv nutrient
.import data/food.csv food
# .import food_nutrient.csv food_nutrient
.schema food
.schema nutrient
# .schema food_nutrient
# .quit